# Sample gitconfig
#
# You probably want to change the [user] section here since you are not me.

[hub]
        protocol = https
[user]
        name = Aki Saarinen
        email = aki@akisaarinen.fi
[credential]
        helper = osxkeychain
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        editor = vim
[apply]
        whitespace = nowarn
[help]
        autocorrect = 1
